mv .bashrc $HOME
mv login.py $HOME
rm README.md
cd ~
rm -R termux-login
mkdir $HOME/.termux/ ;echo "extra-keys = [['ESC','/','-','HOME','UP','END','PGUP','clear'],['TAB','CTRL','ALT','LEFT','DOWN','RIGHT','PGDN','exit']]" >> $HOME/.termux/termux.properties
termux-reload-settings
sleep 1
exit && exit
